{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name":"client",
			"image":"prashantthakur036/multi-client",
			"hostname":"client",
			"essential":false
		},
		{
			"name":"server",
                        "image":"prashantthakur036/multi-server",
                        "hostname":"api",
                        "essential":false

		},
		{
			"name":"worker",
                        "image":"prashantthakur036/multi-worker",
                        "hostname":"worker",
                        "essential":false

		},
		{
			"name":"nginx",
                        "image":"prashantthakur036/multi-nginx",
                        "essential":true,
			"portMappings":[
				{
					"hostPort":80,
					"containerPort":80
				}
			],
			"links":["client","server"]
		}

	]
}
